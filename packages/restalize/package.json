{
  "name": "restalize",
  "version": "1.1.5",
  "description": "Robust REST API generator. Generates a full typescript express-js REST API project from just a relational database connection",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:kennasoft/restalize.git"
  },
  "keywords": [
    "rest",
    "rest-api",
    "auto-generated rest api",
    "db to api"
  ],
  "bin": {
    "restalize": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist/",
    "build": "ncc build ./index.ts -o dist/",
    "prerelease": "yarn clean",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "prepare": "yarn release",
    "start": "rimraf ./output && yarn build && npx ./dist/index.js",
    "deploy": "yarn build && npm publish",
    "version:patch": "yarn version --patch",
    "version:minor": "yarn version --minor",
    "version:major": "yarn version --major"
  },
  "author": "Ikenna Okonkwo<ikenna.okonkwo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "restalize-template": "^1.0.0",
    "chalk": "^4.0.0",
    "commander": "^5.1.0",
    "cpy": "^8.1.0",
    "cross-spawn": "^7.0.2",
    "make-dir": "^3.1.0",
    "prompts": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.1",
    "@types/jest": "^25.2.2",
    "@types/node": "12.12.2",
    "@types/prompts": "^2.0.8",
    "@types/rimraf": "^3.0.0",
    "@zeit/ncc": "0.22.1",
    "jest": "^26.0.1",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "peerDependencies": {
    "typeorm-model-generator": "^0.4.2"
  }
}
