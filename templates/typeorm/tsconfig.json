{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "strictPropertyInitialization": false,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "rootDir": "./server"
  },
  "exclude": ["./dist"]
}
